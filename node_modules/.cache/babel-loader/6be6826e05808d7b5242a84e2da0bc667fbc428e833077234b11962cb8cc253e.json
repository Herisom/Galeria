3.jpg'],\n};\n\nfunction App() {\n  const [category, setCategory] = useState('nature');\n  const [selectedImage, setSelectedImage] = useState(null);\n\n  return (\n    <div className=\"App\">\n      <h1>Galeria zdjęć</h1>\n\n      <div className=\"buttons\">\n        <button onClick={() => setCategory('nature')}>Natura</button>\n        <button onClick={() => setCategory('city')}>Miasto</button>\n        <button onClick={() => setCategory('animals')}>Zwierzęta</button>\n      </div>\n\n      <div className=\"gallery\">\n        {images[category].map((img, idx) => (\n          <img\n            key={idx}\n            src={`${process.env.PUBLIC_URL}/images/${img}`}\n            alt={img}\n            className=\"thumbnail\"\n            onClick={() => setSelectedImage(`${process.env.PUBLIC_URL}/images/${img}`)}\n          />\n        ))}\n      </div>\n\n      {selectedImage && (\n        <div className=\"modal\" onClick={() => setSelectedImage(null)}>\n          <img src={selectedImage} alt=\"Powiększone\" className=\"modal-image\" />\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport default App;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,MAAO,WAAW,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEnB,KAAM,CAAAC,MAAM,CAAG,CACbC,MAAM,CAAE,CAAC,aAAa,CAAE,aAAa,CAAE,aAAa,CAAC,CACrDC,IAAI,CAAE,CAAC,WAAW,CAAE,WAAW,CAAE,WAAW,CAAC,CAC7CC,OAAO,CAAE,CAAC,cAAc,CAAE,cAAc,CAAE,cAAc,CAC1D,CAAC,CAED,QAAS,CAAAC,GAAGA,CAAA,CAAG,CACb,KAAM,CAACC,QAAQ,CAAEC,WAAW,CAAC,CAAGX,QAAQ,CAAC,QAAQ,CAAC,CAClD,KAAM,CAACY,aAAa,CAAEC,gBAAgB,CAAC,CAAGb,QAAQ,CAAC,IAAI,CAAC,CAExD,mBACEI,KAAA,QAAKU,SAAS,CAAC,KAAK,CAAAC,QAAA,eAClBb,IAAA,OAAAa,QAAA,CAAI,yBAAa,CAAI,CAAC,cAEtBX,KAAA,QAAKU,SAAS,CAAC,SAAS,CAAAC,QAAA,eACtBb,IAAA,WAAQc,OAAO,CAAEA,CAAA,GAAML,WAAW,CAAC,QAAQ,CAAE,CAAAI,QAAA,CAAC,QAAM,CAAQ,CAAC,cAC7Db,IAAA,WAAQc,OAAO,CAAEA,CAAA,GAAML,WAAW,CAAC,MAAM,CAAE,CAAAI,QAAA,CAAC,QAAM,CAAQ,CAAC,cAC3Db,IAAA,WAAQc,OAAO,CAAEA,CAAA,GAAML,WAAW,CAAC,SAAS,CAAE,CAAAI,QAAA,CAAC,gBAAS,CAAQ,